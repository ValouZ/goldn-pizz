\r\n  z-index: 25;\r\n  font-size: 20px;\r\n\r\n  nav {\r\n    @include flex($justify: space-around, $align: center);\r\n    height: var(--menu-height);\r\n    width: 100vw;\r\n    background: var(--yellow);\r\n\r\n    .home {\r\n      @include flex($justify: center, $align: center);\r\n      width: 39px;\r\n      height: 39px;\r\n      background: white;\r\n      border-radius: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n.goldn-pizz {\r\n  margin-top: 26px;\r\n  font-size: 36px;\r\n}\r\n","styles\\home\\searchbar.scss":".searchbar {\r\n  position: relative;\r\n  margin-top: 13px;\r\n  margin-bottom: 30px;\r\n\r\n  label {\r\n    @include pAbsolute($left: 20px, $top: 50%);\r\n    transform: translateY(-50%);\r\n    height: 25px; \r\n  }\r\n  input {\r\n    width: 315px;\r\n    height: 40px;\r\n    background: var(--yellow);\r\n    border: none;\r\n    border-radius: 50px;\r\n    box-shadow: 0px 4px 20px rgba(255, 209, 102, 0.4);\r\n    padding-left: 56px;\r\n    font-size: 24px;\r\n    font-weight: 300;\r\n    outline: none;\r\n\r\n    &::placeholder {\r\n      color: var(--placeholder);\r\n    }\r\n  }\r\n}","styles\\home\\cards.scss":".cards{\r\n  @include flex (column, center, center);\r\n  margin-bottom: 60px;\r\n\r\n  .card:not(:last-child){\r\n    margin-bottom: 23px;\r\n  }\r\n}\r\n\r\n.card {\r\n  @include flex($align: center);\r\n  position: relative;\r\n  padding-left: 15px;\r\n  width: 350px;\r\n  height: 135px;\r\n  @include box-shadow();\r\n  border-radius: 19px;\r\n  background: white;\r\n\r\n  &::after {\r\n    content: \"\";\r\n    @include pAbsolute($right: 15px, $top: 23px);\r\n    background: url(\"~/assets/images/arrow.svg\");\r\n    width: 10px;\r\n    height: 10px;\r\n  }\r\n\r\n  img {\r\n    margin-right: 25px;\r\n    height: 111px;\r\n    width: 111px;\r\n    border-radius: 200px;\r\n  }\r\n\r\n  .content {\r\n    width: 180px;\r\n\r\n    .pizza-name {\r\n      font-size: 24px;\r\n      letter-spacing: 0.6px;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .description {\r\n      font-size: 14px;\r\n      color: var(--text-description);\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    .price {\r\n      font-size: 18px;\r\n      span {\r\n        font-size: 14px;\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"855f00b8772e0fcddfd9e318b99c4d11","cacheData":{"env":{}}}